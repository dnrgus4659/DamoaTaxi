create table zipcode(
	ZIPCODE varchar(100) not null,
	SIDO varchar(100) not null,
	GUGUN varchar(200) not null,
	DONG varchar(200) not null,
	RI varchar(500) null,
	BUNJI varchar(300) null,
	SEQ int null
);

desc zipcode;

select * from zipcode;

delete from zipcode;
drop table zipcode;